package boletin7_5;

import javax.swing.JOptionPane;

/**
 *
 * @author dam1
 */
public class Boletin7_5 {

    public static void main(String[] args) {

        String auxNum1 = JOptionPane.showInputDialog("Introduce o primero número: ");
        int num1 = Integer.parseInt(auxNum1);
        String auxNum2 = JOptionPane.showInputDialog("Introduce o segundo número: ");
        int num2 = Integer.parseInt(auxNum2);
        String auxNum3 = JOptionPane.showInputDialog("Introduce o terceiro número: ");
        int num3 = Integer.parseInt(auxNum3);

        if (num1 <= num2 && num2 > num3) {

            JOptionPane.showMessageDialog(null, "O primeiro número é o maior: " + num1);

        } else {
            if (num2 > num3) {

                JOptionPane.showMessageDialog(null, "O segundo número é o maior: " + num2);

            }
            JOptionPane.showMessageDialog(null, "O terceiro número é o maior: " + num3);

        }
    }

}
